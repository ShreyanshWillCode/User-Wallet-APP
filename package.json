{
  "name": "simple-user-wallet",
  "version": "1.0.0",
  "description": "A full-stack Simple User Wallet Web App with Node.js, Express, MongoDB, and React",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "vite",
    "backend:install": "cd backend && npm install",
    "frontend:install": "npm install",
    "install:all": "npm run backend:install && npm run frontend:install",
    "build": "vite build",
    "start": "cd backend && npm start",
    "test": "npm run backend:test && npm run frontend:test",
    "backend:test": "cd backend && npm test",
    "frontend:test": "vitest"
  },
  "keywords": [
    "wallet",
    "fintech",
    "nodejs",
    "express",
    "mongodb",
    "react",
    "tailwindcss",
    "fullstack"
  ],
  "author": "Wallet App Team",
  "license": "MIT",
